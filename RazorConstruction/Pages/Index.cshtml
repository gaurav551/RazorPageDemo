                                        <img src="/assets/img/slider/transparent-slide.png" alt="">
                                    </div>
                               </div>
                           </div>
                       </div>
                    </div>
                </div>
            </div>
            <!-- slider end -->
            <!-- features-area start -->
            <div class="features-area pt-100 pb-70">
                <div class="container">
                    <div class="row">
                        <div class="col-xl-12">
                            <div class="features-title text-center mb-80">
                                <h2>Our Services <br>  <span>to lead your business.</span></h2>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        @foreach (var x in Model.Services)
                        {
                            
                        
                        <div class="col-xl-4 col-lg-4 col-md-4">
                            <div class="single-features text-center mb-30">
                               
                                <h2>@x.Title</h2>
                                <p>@x.Description</p>
                            </div>
                        </div>
                        }
                        
                    </div>
                </div>
            </div>
            <!-- features-area start -->
            
            <!-- features-area end -->
            <!-- portfolio-area start -->
            <div class="portfolio-area pt-100 pb-100">
                <div class="container-full">
                    <div class="row no-gutters">
                        <div class="col-xl-12">
                            <div class="section-title section-title-2 text-center mb-50">
                                <h2>Recent Projects</h2>
                            </div>
                        </div>
                    </div>
                    <div class="row no-gutters">
                        <div class="col-xl-12">
                            <div class="portfolio-menu mb-40 text-center">
                                <button class="active" data-filter="*">ALL</button>
                                <button data-filter=".Building">BUILDINGS</button>
                               
                                <button data-filter=".House">HOUSE</button>
                                <button data-filter=".Interior">INTERIOR</button>
                                <button data-filter=".Office">OFFICE</button>
                                 <button data-filter=".Othersw">Others</button>
                            </div>
                        </div>
                    </div>
                    <div class="row no-gutters grid">
                        @foreach (var x in Model.Projects)
                        {
                            
                        
                        <div class="col-xl-4 col-lg-4 col-md-6 grid-item @x.Category">
                            <div class="portfolio-wrapper">
                                <div class="portfolio-thumb">
                                    <img src="/projects/@x.PhotoUrl" alt="" width="200px" height="300px">
                                </div>
                                <div class="portfolio-content">
                                    <div class="icon"><a class="popup-img" href="/projects/@x.PhotoUrl"><span></span></a></div>
                                    <h3><a href="javascript:void()">@x.Name</a></h3>
                                    <p><a href="javascript:void()">@x.Description</a></p>
                                    <br>
                                    <span>Project Category : @x.Category</span>
                                    <br>
                                    <span>Status : @x.Status</span>
                                    <br>
                                    <span>Client : @x.Client</span>


                                </div>
                            </div>
                        </div>
                        }
                    
                    </div>
                    <div class="row no-gutters">
                       <div class="col-xl-12">
                            <div class="videw-project text-center mt-50">
                                <a href="#" class="btn brand-btn">ALL PROJECTS</a>
                            </div>
                       </div>
                    </div>
                </div>
            </div>
            <!-- portfolio-area end -->
            <div>
                <div class="container">
                    <div class="row">
                       
                        <div class="col-xl-12 col-lg-12 col-md-12">
                            <div class="promotion-text promo-2 pt-10 pb-110">
                                <h3>Don’t Wait For
                                    <br> anything. Buy it today!</h3>
