                                    ut aliquip ex ea com modo consequat. Duis aute irure.</P>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
             <!-- team-area strat -->
            <div class="team-area pb-70">
                <div class="container">
                    <div class="row">
                        <div class="col-xl-12">
                            <div class="section-title section-title-2 text-center mb-70">
                                <h2>Meet Our Experts</h2>
                                <p>We have a team of highly dedicated engineers working to serve the quality service for your needs</p>
                                <a href="/careers">Apply for a job ></a>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        @foreach (var x in Model.Members)
                        {
                            
                        
                        <div class="col-xl-3 col-lg-3 col-md-6">
                            <div class="single-team text-center mb-30">
                                <div class="team-img">
                                    <img src="/members/@x.PhotoUrl" alt="">
                                    <div class="team-icon">
                                        <a href="#">
                                            <i class="fab fa-facebook-f"></i>
                                        </a>
                                        <a href="#">
                                            <i class="fab fa-twitter"></i>
                                        </a>
                                        <a href="#">
                                            <i class="fab fa-google-plus-g"></i>
                                        </a>
                                        <a href="#">
                                            <i class="fab fa-instagram"></i>
                                        </a>
                                    </div>
                                </div>
                                <div class="team-info">
                                    <h3>@x.Name</h3>
                                    <span>@x.Designation</span>
                                </div>
                            </div>
                        </div>
                        }
                        
                    </div>
                </div>
            </div>
            <!-- team-area end -->
